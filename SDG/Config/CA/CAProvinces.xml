<?xml version="1.0" encoding="utf-8"?>
<generators>
	<generator
		type="RedGate.SQLDataGenerator.ExpressionGenerators.FileListGenerator"
		name="Canadian Provinces"
		description="Canadian Provinces and Territories"
		category="Geographical">

		<property name="FileName">CAProvinces.txt</property>

		<matches field="Provinces" score="50" minlen="25"/>
		<matches field="Addr.*2" score="30" minlen="25"/>
		<matches field="Addr.*[34]" score="20" minlen="25"/>
		<type type="string"/>
	</generator>
</generators>

